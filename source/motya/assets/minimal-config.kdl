system {
    threads-per-service 8
    daemonize #false 
    pid-file "/tmp/motya.pidfile"
    upgrade-socket "/tmp/motya-upgrade.sock"
}

services {
    Example {
        listeners {
            "0.0.0.0:8080"
        }
        connectors {
            return code="200" response="OK"
        }
    }
}

